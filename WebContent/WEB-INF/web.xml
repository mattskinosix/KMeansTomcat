<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>matteo</display-name>
	
	<servlet>
		<servlet-name>LearnFromDb</servlet-name>
		<servlet-class>LearnFromDb
		</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>LearnFromDb</servlet-name>
		<url-pattern>/LearnFromDb</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LearnFromFile</servlet-name>
		<servlet-class>LearnFromFile
		</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>LearnFromFile</servlet-name>
		<url-pattern>/LearnFromFile</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>